{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "deleteOutDir": true,
    "tsConfigPath": "tsconfig.json",
    "plugins": [
      "@nestjs/swagger"
    ],
    "assets": [
      {
        "include": ".env",
        "outDir": "dist/"
      },
      {
        "include": "prisma/**/*",
        "outDir": "dist/prisma"
      }
    ]
  }
}